    "@types/mapbox__polyline": "^1.0.5",
    "@types/papaparse": "^5.3.14",
    "@types/polyline": "^0.1.32",
    "@types/react-table": "^7.7.20",
    "@types/react-window": "^1.8.8",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.3",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-zoom": "^2.0.1",
    "cookie-session": "^2.1.0",
